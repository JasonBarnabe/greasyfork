//Store data locally
localprofitDB[combatId]={timestamp:timestamp,attackerid:attackerid,defenderid:defenderid,coordinates:coordinates,loot:loot,df:df,attackerloss:attackerloss,defenderloss:defenderloss,repaired:repair,wreckfield:wreckfield},localStorage.setItem('NRT_profitDB',JSON.stringify(localprofitDB))}
