//parse DF message based on HTML element
function parseDF(msg){var date,content=msg.getElementsByClassName('msg_content')[0].innerHTML.substring(msg.getElementsByClassName('msg_content')[0].innerHTML.search("]</a>"),msg.getElementsByClassName('msg_content')[0].innerHTML.length),apiKey=msg.innerHTML.substring(msg.innerHTML.search("input value='")+13,msg.innerHTML.search("' readonly")),numbers=content.match(/((\d)*(\.)*(\d))+/g),crystal=parseInt(numbers[numbers.length-1].replace(/\./g,"")),metal=parseInt(numbers[numbers.length-2].replace(/\./g,"")),day=(date=msg.getElementsByClassName("msg_date fright")[0].hasAttribute("original")?msg.getElementsByClassName("msg_date fright")[0].getAttribute("original").split("."):msg.getElementsByClassName('msg_date fright')[0].innerHTML.split("."))[0],month=date[1]-1,year=date[2].split(" ")[0],hours=date[2].split(" ")[1].split(":")[0],minutes=date[2].split(" ")[1].split(":")[1],seconds=date[2].split(" ")[1].split(":")[2];
//create date that is in unix time
date=new Date(year,month,day,hours,minutes,seconds,0).getTime()/1e3;var coords=msg.getElementsByClassName('txt_link')[0].innerHTML.replace("[","").replace("]","");
//Store data locally if metal or crystal are not both 0
metal+crystal>0&&(localdfDB[apiKey]={timestamp:date,coordinates:coords,metal:metal,crystal:crystal},localStorage.setItem('NRT_dfDB',JSON.stringify(localdfDB)))}
//Parse combat report based on message ID
function parseCR(id){
//Try get the html data from the detailed window
try{$.ajax({type:'GET',url:'index.php?page=messages',data:'messageId='+id+'&tabid=21&ajax=1',dataType:'html',context:document.body,global:!1,async:!0,error:function(jqXHR,exception){console.log("Error getting HTML data - check errorlog")},success:function(data){
//Try parse the html data
try{var div=document.createElement('div');if(div.id="workspace",div.style.visibility="hidden",document.body.appendChild(div),div){div.innerHTML=data;var combatreport=div.getElementsByClassName('detail_msg')[0];if(combatreport&&combatreport.getElementsByClassName("msg_actions clearfix")[0].innerHTML.length>300){
