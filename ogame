for(var dkey in json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses)if(void 0!==json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey])for(var dshiploss in json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey])"undefined"!=json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey][dshiploss]&&(defenderloss[dshiploss]+=parseInt(json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey][dshiploss]));
//For each defender - add their ID to defender list
for(var defkey in json.defender)defenderid+=json.defender[defkey].ownerID+",";combatId=combatreport.getElementsByClassName("msg_actions clearfix")[0].innerHTML.substring(combatreport.getElementsByClassName("msg_actions clearfix")[0].innerHTML.search("<br/><input value='")+19,combatreport.getElementsByClassName("msg_actions clearfix")[0].innerHTML.search("' readonly"))}
//If expedition, set attackerID to expo and set defenderID
else{for(var defkeys in
//console.log(json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses);
attackerid="expo",json.defender)defenderid+=json.defender[defkeys].ownerID+",";for(var dkey2 in json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses)if(void 0!==json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey2])for(var dshiploss2 in json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey2])"undefined"!=json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey2][dshiploss2]&&(defenderloss[dshiploss2]+=parseInt(json.defenderJSON.combatRounds[json.defenderJSON.combatRounds.length-1].losses[dkey2][dshiploss2]))}
//If no losses - create empty arrays
